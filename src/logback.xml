<configuration>
  <appender name="FILE" class="ch.qos.logback.core.FileAppender">    
    <File>Sparql1.1_4j.log</File>
    <encoder>
      <pattern>%d{yyyy-MM-dd HH:mm:ss} %c [%p] %m%n</pattern>
    </encoder>
  </appender>
  <root level="WARN">
    <appender-ref ref="FILE"/>    
  </root>
</configuration>
